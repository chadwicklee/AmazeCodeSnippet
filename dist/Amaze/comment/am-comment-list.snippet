        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
      <FileExtensions>
        aspx;master;ascx;html
      </FileExtensions>
      <Shortcut>am-comment:list</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>评论者</ID>
          <Type />
          <ToolTip>评论者</ToolTip>
          <Default>评论者</Default>
          <Function />
        </Literal>
