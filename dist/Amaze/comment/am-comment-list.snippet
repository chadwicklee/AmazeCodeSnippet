<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.1.0">
    <Header>
      <Title>am-comment:list</Title>
      <Author>南方网景</Author>
      <Description>评论组件：综合</Description>
      <HelpUrl>http://amazeui.org/css/comment</HelpUrl>
      <SnippetTypes>
