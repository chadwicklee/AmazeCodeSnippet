<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.1.0">
    <Header>
      <Title>am-cdn</Title>
      <Author>南方网景</Author>
      <Description>CDN：CSS+JS</Description>
      <HelpUrl>http://amazeui.org/getting-started</HelpUrl>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
      <FileExtensions>
        aspx;master;ascx;html
      </FileExtensions>
      <Shortcut>am-cdn</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
      </Declarations>
      <Code Language="html" Kind="" Delimiter="$"><![CDATA[
<!-- Latest compiled and minified CSS and JS -->
<link rel="stylesheet" href="//cdn.amazeui.org/amazeui/2.2.1/css/amazeui.min.css">
<script src="//libs.useso.com/js/jquery/2.1.1/jquery.min.js"></script>
<script src="//cdn.amazeui.org/amazeui/2.2.1/js/amazeui.min.js"></script>
]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
