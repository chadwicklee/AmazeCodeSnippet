<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.1.0">
    <Header>
      <Title>am-ucheck-radio</Title>
      <Author>南方网景</Author>
      <Description>单选框：radio-默认</Description>
      <HelpUrl>http://amazeui.org/javascript/ucheck</HelpUrl>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
      <FileExtensions>
        aspx;master;ascx;html
      </FileExtensions>
      <Shortcut>am-ucheck-radio</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal Editable="true">
          <ID>radio</ID>
          <Type />
          <ToolTip>radio</ToolTip>
          <Default>radio</Default>
          <Function />
        </Literal>
        <Literal Editable="true">
          <ID>title</ID>
          <Type />
          <ToolTip>title</ToolTip>
          <Default>title</Default>
          <Function />
        </Literal>
      </Declarations>
      <Code Language="html" Kind="" Delimiter="$">
        <![CDATA[
<label class="am-radio">
  <input type="$selected$$radio$$end$" value="" data-am-ucheck> $selected$$title$$end$
</label>
]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
